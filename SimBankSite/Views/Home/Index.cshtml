@model SimBankSite.Models.ApplicationUser

@{
    ViewBag.Title = "Home Page";
}

<div class="row">
    <div class="col-md-4">
        <h3>Выберите сервис, для которого Вам нужно принять СМС.</h3>
        <form method="post" action="">
            <input type="radio" name="vk" value="Vk" class="radioChooseEntity" /> Вконтакте <br />
            <input type="radio" name="ok" class="radioChooseEntity" /> Одноклассники <br />
            <input type="radio" name="steam" class="radioChooseEntity" /> Steam <br />
            <input type="radio" name="uber" class="radioChooseEntity" /> Uber <br />
            <input type="submit" class="btn-lg btn-success" />
        </form>
    </div>

    <div class="col-md-8">
        <div class="jumbotron">
            <h1>Sim Sim SMS</h1>
            <p class="lead">Вы сможете получить смс на наш номер</p>
            <p><a href="/Home/Help" class="btn btn-primary btn-lg">Раздел помощи &raquo;</a></p>
            <table>
                <h3>
                    @foreach (SimBankSite.Models.ApplicationUser u in ViewBag.Users)
                    {

                        <tr>
                            <td>@u.UserName</td>
                            <td>@u.Id </td>
                            
                        </tr>


                    }
                </h3>
            </table>
        </div>
    </div>
</div>
